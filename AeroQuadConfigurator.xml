<Configurator>

	<Settings>
		<DefaultComPort>COM5</DefaultComPort>
		<BootUpDelay>0.0</BootUpDelay>
		<CommTimeOut>1.0</CommTimeOut>
		<DefaultBaudRate>115200</DefaultBaudRate>
		<AvailableBaudRates>1200,9600,38400,57600,111111,115200</AvailableBaudRates>
		<SoftwareVersion>!</SoftwareVersion>
		<StopTelemetry>X</StopTelemetry>
		<BoardConfiguration>#</BoardConfiguration>
	</Settings>
	
	<Boards>
		<Type>AeroQuad32</Type>
		<Type>AeroQuad v2.1</Type>
		<Type>AeroQuad v1.9</Type>
		<Type>AeroQuad Mini v1.05</Type>
		<Selected>AeroQuad32</Selected>
	</Boards>
	
	<Board Type="AeroQuad32">
		<Subpanels>
			<Subpanel Name="Update Parameters">
				<Path>updateParameters.updateParameters</Path>
				<Class>updateParameters</Class>
				<ParameterType Name="Acrobatic (Rate) Mode">
					<Requirement>Gyroscope: Detected</Requirement>
					<Command>A</Command>
					<Telemetry>a</Telemetry>
					<UpdateEEPROM>W</UpdateEEPROM>
					<Parameter Name="Roll Proportional" Description="">3.0</Parameter>
					<Parameter Name="Roll Integral" Description="">0.0</Parameter>
					<Parameter Name="Roll Derivative" Description="">-100.0</Parameter>
					<Parameter Name="Pitch Proportional" Description="">3.0</Parameter>
					<Parameter Name="Pitch Integral" Description="">0.0</Parameter>
					<Parameter Name="Pitch Derivative" Description="">-100.0</Parameter>
					<Parameter Name="Rotation Speed" Description="">5</Parameter>
				</ParameterType>
				<ParameterType Name="Stable (Attitude) Mode">
					<Requirement>Gyroscope: Detected</Requirement>
					<Requirement>Accelerometer: Detected</Requirement>
					<Command>B</Command>
					<Telemetry>b</Telemetry>
					<UpdateEEPROM>W</UpdateEEPROM>
					<Parameter Name="Roll Accel Proportional" Description="">4.0</Parameter>
					<Parameter Name="Roll Accel Integral" Description="">0.0</Parameter>
					<Parameter Name="Roll Accel Derivative" Description="">-100.0</Parameter>
					<Parameter Name="Pitch Accel Proportional" Description="">4.0</Parameter>
					<Parameter Name="Pitch Accel Integral" Description="">0.0</Parameter>
					<Parameter Name="Pitch Accel Derivative" Description="">-100</Parameter>
					<Parameter Name="Roll Gyro Proportional" Description="">3.0</Parameter>
					<Parameter Name="Roll Gyro Integral" Description="">0.0</Parameter>
					<Parameter Name="Roll Gyro Derivative" Description="">-200</Parameter>
					<Parameter Name="Pitch Gyro Proportional" Description="">3.0</Parameter>
					<Parameter Name="Pitch Gyro Integral" Description="">0.0</Parameter>
					<Parameter Name="Pitch Gyro Derivative" Description="">-200</Parameter>
					<Parameter Name="Windup Guard" Description="">1000</Parameter>
				</ParameterType>
				<ParameterType Name="Yaw Configuration">
					<Requirement>Gyroscope: Detected</Requirement>
					<Command>C</Command>
					<Telemetry>c</Telemetry>
					<UpdateEEPROM>W</UpdateEEPROM>
					<Parameter Description="" Name="Yaw Proportional">5.0</Parameter>
					<Parameter Description="" Name="Yaw Integral">0.0</Parameter>
					<Parameter Description="" Name="Yaw Derivative">0.0</Parameter>
					<Parameter Description="" Name="Heading Hold Proportional">2.0</Parameter>
					<Parameter Description="" Name="Heading Hold Integral">0.0</Parameter>
					<Parameter Description="" Name="Heading Hold Derivative">0.0</Parameter>
					<Parameter Description="1 = Enabled, 0 = Disabled" Name="Heading Hold Configuration">1</Parameter>
				</ParameterType>
			</Subpanel>
			<Subpanel Name="Serial Monitor">
				<Path>commMonitor.commMonitor</Path>
				<Class>commMonitor</Class>
			</Subpanel>
			<Subpanel Name="Sensor Data">
				<Path>dataPlot.dataPlot</Path> 
				<Class>dataPlot</Class> 
				<Telemetry>i</Telemetry>
				<Index>0</Index>
				<PlotSize>128</PlotSize>
				<PlotName>Gyro X Axis</PlotName>
				<PlotName>Gyro Y Axis</PlotName>
				<PlotName>Gyro Z Axis</PlotName>
				<PlotName>Accel X Axis</PlotName>
				<PlotName>Accel Y Axis</PlotName>
				<PlotName>Accel Z Axis</PlotName>
				<PlotName>Mag X Axis</PlotName>
				<PlotName>Mag Y Axis</PlotName>
				<PlotName>Mag Z Axis</PlotName>
			</Subpanel>
			<Subpanel Name="Gyroscope Data">
				<Path>dataPlot.dataPlot</Path> 
				<Class>dataPlot</Class> 
				<Telemetry>i</Telemetry>
				<Index>0</Index>
				<PlotSize>128</PlotSize>
				<PlotName>Gyro X Axis</PlotName>
				<PlotName>Gyro Y Axis</PlotName>
				<PlotName>Gyro Z Axis</PlotName>
			</Subpanel>
			<Subpanel Name="Accelerometer Data">
				<Path>dataPlot.dataPlot</Path> 
				<Class>dataPlot</Class> 
				<Telemetry>i</Telemetry>
				<Index>3</Index>
				<PlotSize>128</PlotSize>
				<PlotName>Accel X Axis</PlotName>
				<PlotName>Accel Y Axis</PlotName>
				<PlotName>Accel Z Axis</PlotName>
			</Subpanel>
			<Subpanel Name="Magnetometer Data">
				<Path>dataPlot.dataPlot</Path> 
				<Class>dataPlot</Class> 
				<Telemetry>i</Telemetry>
				<Index>6</Index>
				<PlotSize>128</PlotSize>
				<PlotName>Mag X Axis</PlotName>
				<PlotName>Mag Y Axis</PlotName>
				<PlotName>Mag Z Axis</PlotName>
			</Subpanel>
		</Subpanels>
	</Board>
